@model IEnumerable<stFacturacion.Models.AlbaranCompra>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            albaranDate
        </th>
        <th>
            proveedor
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.albaranDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.proveedor.nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.albaranCompraId }) |
            @Html.ActionLink("Details", "Details", new { id=item.albaranCompraId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.albaranCompraId })
        </td>
    </tr>
}

</table>
